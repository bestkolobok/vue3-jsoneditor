{
  "name": "vue3-ts-jsoneditor",
  "version": "3.1.2",
  "author": {
    "name": "Alex Guzey",
    "email": "alex.guzey@gmail.com"
  },
  "private": false,
  "license": "MIT",
  "type": "module",
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "./dist/JsonEditorPlugin.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/JsonEditorPlugin.d.ts",
      "default": "./dist/index.mjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bestkolobok/vue3-jsoneditor.git"
  },
  "bugs": {
    "url": "https://github.com/bestkolobok/vue3-jsoneditor/issues"
  },
  "homepage": "https://github.com/bestkolobok/vue3-jsoneditor#readme",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "build:gh": "VITECONFIG_build_lib=false VITECONFIG_build_rollupOptions=false VITECONFIG_build_cssMinify=esbuild VITECONFIG_build_outDir=gh-dist VITECONFIG_base='/vue3-jsoneditor' run-p build-only && touch gh-dist/.nojekyll",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --fix",
    "test": "echo \"No tests yet\"",
    "semantic-release": "semantic-release",
    "deploy:local": "npm run build && npm run build:gh && npm publish",
    "pack": "npm pack",
    "prepare": "husky"
  },
  "dependencies": {
    "immutable-json-patch": "^6.0.1",
    "vanilla-jsoneditor": "^3.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.38.0",
    "@rushstack/eslint-patch": "^1.1.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.2",
    "@semantic-release/npm": "^13.1.2",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/node": "^22.14.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.38.0",
    "eslint-plugin-vue": "^9.33.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "sass": "^1.86.3",
    "semantic-release": "^25.0.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.46.1",
    "vite": "^7.2.6",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-env-config": "^2.0.1",
    "vue-tsc": "^3.1.1"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "keywords": [
    "vue",
    "jsoneditor",
    "json",
    "editor"
  ],
  "funding": [
    "https://u24.gov.ua/",
    "https://savelife.in.ua/en/"
  ]
}
